ALERT ID alRomIncompatible
ERROR
BEGIN
	TITLE "System Incompatible"
	MESSAGE "System Version 5.0 or\ngreater is required to\nrun Universe."
	BUTTONS "OK"
END

ALERT ID alDebug
INFORMATION
BEGIN
	TITLE "Universe Debug"
	MESSAGE "File: ^1\n" \
                "Line: ^2\n" \
                "Data: ^3\n" \
                "Please write this down and send it to support@wapuniverse.com."
	BUTTONS "Yep!"
END

