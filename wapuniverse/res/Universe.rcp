MENU ID mnuOptions
    BEGIN
    PULLDOWN "Options"
        BEGIN
        MENUITEM "About" ID OptionsGetInfo "I"
        MENUITEM "Build Info..." ID OptionsBuild "F"
	MENUITEM SEPARATOR
	MENUITEM "Donate" ID OptionsDonate
	MENUITEM SEPARATOR
        MENUITEM "Bookmarks" ID OptionBookmarks "B"
        MENUITEM "Connection Settings" ID OptionConnectionSettings "C"
	MENUITEM "Preferences" ID OptionsPrefs "G" /* Working on prefs */
        MENUITEM "Network Prefs" ID OptionNetwork "N"
        END
    END

MENU ID mnuPush
    BEGIN
    PULLDOWN "Help"
        BEGIN
        MENUITEM "Push Help" ID PushPushHelp
        END
    END

MENU ID mnuEdit
    BEGIN
    PULLDOWN "Edit"
        BEGIN
        MENUITEM "Undo" ID EditEditUndo "U"
        MENUITEM "Cut" ID EditEditCut "X"
        MENUITEM "Copy" ID EditEditCopy "C"
        MENUITEM "Paste" ID EditEditPaste "P"
	MENUITEM SEPARATOR
        MENUITEM "Keyboard" ID EditEditKeyboard "K"
        MENUITEM "Graffiti Help" ID EditEditGraffiti "G"
        END
    END

MENU ID mnuView
BEGIN
	PULLDOWN "File"
	BEGIN
		MENUITEM "Toggle Private Browsing" ID miPrivateToggle
		MENUITEM "Private Browsing Options..." ID miPrivate
		MENUITEM SEPARATOR
		MENUITEM "New Tab" ID miNewTab
		MENUITEM "Close Tab" ID miCloseTab
		MENUITEM "Open URL..." ID miOpenUrl
		/*MENUITEM "Open Location..." ID miOpenLocation*/
